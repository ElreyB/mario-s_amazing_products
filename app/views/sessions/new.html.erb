<div class="signin-container">
  <div class="well signin">
    <h1>Sign in</h1>
    <%= form_tag signin_path do %>
      <div class="form-group">
        <%= label_tag :email %>
        <br>
        <%= text_field_tag :email%>
      </div>
      <div class="form-group">
        <%= label_tag :password %>
        <br>
        <%= password_field_tag :password%>
      </div>
        <%= submit_tag "Sign in", class: "btn btn-block"%>
    <%end %>

  </div>
</div>
